{
  "name": "logsmith",
  "version": "0.1.0",
  "description": "A toolkit for transforming and working with Logseq data",
  "main": "src/index.js",
  "bin": {
    "logsmith": "src/cli/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "start": "node src/cli/index.js",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "husky install",
    "build": "mkdir -p dist && cp -r src/* dist/",
    "prepublishOnly": "npm run test && npm run lint && npm run build"
  },
  "keywords": [
    "logseq",
    "markdown",
    "converter",
    "notes",
    "knowledge-base"
  ],
  "author": "Sid",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidsarasvati/logseq-utils.git"
  },
  "bugs": {
    "url": "https://github.com/sidsarasvati/logseq-utils/issues"
  },
  "homepage": "https://github.com/sidsarasvati/logseq-utils#readme",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.1",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}