{
  "name": "logseq-to-md",
  "version": "0.1.0",
  "description": "Convert Logseq format to standard Markdown",
  "main": "src/index.js",
  "bin": {
    "logseq-to-md": "src/cli/index.js"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "start": "node src/cli/index.js",
    "format": "prettier --write \"src/**/*.js\"",
    "prepare": "husky install"
  },
  "keywords": [
    "logseq",
    "markdown",
    "converter",
    "notes",
    "knowledge-base"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^9.4.1",
    "fs-extra": "^11.1.0",
    "glob": "^8.1.0",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
